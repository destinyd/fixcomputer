/*
 * This is a manifest file that'll be compiled into application.css, which will include all the files
 * listed below.
 *
 * Any CSS and SCSS file within this directory, lib/assets/stylesheets, vendor/assets/stylesheets,
 * or vendor/assets/stylesheets of plugins, if any, can be referenced here using a relative path.
 *
 * You're free to add application-wide styles to this file and they'll appear at the top of the
 * compiled file, but it's generally better to create a new file per style scope.
 *
 *= require_self
 *= require_tree .
*/

a {
  color: #0088cc;
  text-decoration: none;
}

article, aside, details, figcaption, figure, footer, header, hgroup, nav, section {
  display: block;
}

::selection {
  background: #FF44ae;
  color: #fff;
  text-shadow: none;
}

a {
  color: #00a0c6;
  outline: 0 none;
}

a:link {
  -webkit-tap-highlight-color: #FF44AE;
}

a:visited {
  color: #fff;
}

.wrapper {
  width: 960px;
  margin: 0 auto;
  overflow: hidden;
}

/*.clearfix:before, .clearfix:after {*/
/*display: table;*/
/*content: "";*/
/*line-height: 0;*/
/*}*/

/*.clearfix:after {*/
/*clear: both;*/
/*}*/

.one-half {
  width: 460px;
}

.one-fourth {
  width: 220px;
}

.one-half, .one-third, .one-fourth, .two-third, .three-fourth {
  margin: 0 20px 0 0;
  .last{
    margin: 0 !important;
  }
}

.pull-left, .one-half, .one-third, .one-fourth, .two-third, .three-fourth, #steps .step .imgborder, #steps .step .step-no, #steps .step .step-content {
  float: left;
}

#header {
  position: relative;
  padding-top: 7px;
  background: #00a0c6 url(main_top_line.gif) repeat-x 0 0;
}

.inline-block-list li, #steps #step-nav ul li, #main-menu ul li, #user-nav ul li {
  list-style-image: none;
  list-style-type: none;
  margin-left: 0;
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  zoom: 1;
  white-space: nowrap;
}

#main-menu {
  position: absolute;
  top: 31px;
  right: 0;
  text-align: right;
  font-size: 14px;
  ul li {
    vertical-align: middle;
    height: 54px;
    a {
      display: block;
      padding: 18px 20px 19px;
    }

  }
  li.active a {
    background: transparent url(bg_menu.png) repeat 0 0;
  }
}

#main-menu ul, #user-nav ul {
  margin: 0;
  padding: 0;
  border: 0;
  overflow: hidden;
}

#main-menu ul li a,#main-menu ul li a:visited{color:#fff}
#main-menu ul li a:hover{background-color:#211E1B;text-decoration:none}

#footer{
  clear:both;
  overflow:hidden;
  background-color:#191715;
  font-size:12px;
}
#totop{
  height:54px;
  background:transparent url(bg_totop.png) repeat-x 0 100%;
  a{
    width:39px;
    height:39px;
    padding:0;
    margin:0 auto;
    display:block;
    color:#919191;
    text-decoration:none;
    background:#00a0c6 url(//i_arrow_top.png) no-repeat 100% 0%;
  }
  a:hover{
  background-color:#211e1c}
}
#go-on-top{
  display:inline;
  float:left;
  position:relative;
  margin-left:10px;
  margin-right:10px;
width:940px}
#footer-additional{
  background-color:#1C1917;
  overflow:hidden;
  border-bottom:1px solid #2b2b27;
  .wrapper{
    padding-top:17px;
  }
}
#footer-content{
  background-color:#211E1C;
  overflow:hidden;
  padding-top:60px;
  padding-bottom:40px;
  color:#cccccc;
  h4,h5{
    color:#FFF;
    line-height:100%;
    text-decoration:none;
    padding-bottom:20px;
  }
  .textwidget{
    padding-right:30px;
  }
  .white-dots li a{
    color:#FFF;
    padding:5px 0px 4px 0px;
    display:inline-block;
  }
  .white-dots li a:hover{
    color:#00a0c6;
    text-decoration:none;
  }
  .more{
    font-style:italic;
  }
  .more:hover,.contact li a:hover{
    color:#FFF;
    text-decoration:underline;
  }
  .contact li{
    height:30px;
    color:#00a0c6;
    font-size:14px;
    font-weight:bold;
    padding-left:35px;
  }
  .email{
  background:transparent url(email.png) no-repeat left top}
  .qq{
  background:transparent url(qq.png) no-repeat left top}
  .phone{
    background:transparent url(phone.png) no-repeat left top;
  }

  .social-icons{
    padding:0;
    margin:0;
    border:0;
    overflow:hidden;
    li{
      list-style-image:none;
      list-style-type:none;
      margin-left:0px;
      display:-moz-inline-box;
      -moz-box-orient:vertical;
      display:inline-block;
      vertical-align:middle;
      white-space:nowrap;
      background:none;
      padding:0 2px 5px 0;
    }
    li a:hover{
      opacity:0.8;
      filter:alpha(opacity=80);
    }
  }
}

#share{
  background-color:#f5f5f5;
  padding-bottom:20px;
  .inline-block-list li,#steps #step-nav ul li,#steps #step-nav ul li,#main-menu ul li,#main-menu ul li,#user-nav ul li,#user-nav ul li{
    padding-right:10px;
  }
}

#construction{
  width:350px;
  margin:0 auto;
  text-align:center;
}


#sub-footer{
  overflow:hidden;
min-height:49px}
#copyright{
  padding:16px 0 15px;
  float:left;
color:#6e6d68}
#pricing #teasers{
margin-top:20px !important}

#logo {
  display: inline;
  position: relative;
  margin-left: 10px;
  margin-right: 10px;
  padding: 20px 0 16px;
  float: left;
}

#content {
  overflow: hidden;
  padding: 35px 0px;
}

ul.white-dots li{
  list-style:none;
  padding:1px 0px 1px 13px;
  background:transparent url(bullet.png) no-repeat 0 9px;
}

#nav-bar {
  position: relative;
}
.button-bright-yellow{
  background:#00a0c6 url(yellow_button_bg.png) no-repeat 0 0;
  text-align:center;
  display:inline-block;
  padding:0;
  width:88px;
  height:36px;
  line-height:36px;
  span{
    color:#663300;
    font-size:14px;
    font-style:normal;
  }
}

.button-bright-yellow:hover{
  background-position:0 -40px;
  color:#663300;
  text-decoration:none;
}

#order{
  height:36px;
  .button-bright-yellow{
    padding:0px;
    margin:10px;
  }
  .button-bright-yellow:hover{
    background-color:transparent;
  }
}

.grid-4{
  display:inline;
  float:left;
  position:relative;
  margin-left:10px;
  margin-right:10px;
  width:220px;
}


#teasers .teaser{
  background-color:#EFEFEF;
  h3{
    height:20px;
    padding:30px 20px;
    font-size:18px;
    color:#FFF;
    background-color:#211E1C;
    text-decoration:none;
    line-height:100%;
    text-align:center;
    border-bottom:6px solid #00a0c6;
    -webkit-border-radius:2px 2px 0 0;
    -moz-border-radius:2px 2px 0 0;
    border-radius:2px 2px 0 0;
  }
  .grid-main{
    padding:22px 10px;
    text-align:center;
  height:280px}
  .teaser p{
  line-height:20px}
  .teaser p img{
  margin-bottom:15px}
  .grid-footer{
    border-top:1px solid #e0dedd;
    padding:22px 0 0 0;
    margin:0 20px;
  }
}

#pricing #teasers .teaser{
font-weight:bold;
position:relative;
z-index:1}
#pricing #teasers .teaser .grid-main{
height:auto}
#pricing #teasers .teaser .grid-main p{
font-size:14px;
padding-bottom:20px}
#pricing #teasers .teaser .grid-main .summary{
color:#e49b00}
#pricing #teasers .teaser .grid-main .price{
font-size:24px;
color:#00a0c6}
#pricing #teasers .teaser .highlighted{
position:absolute;
top:-20px;
left:-10px;
width:70px;
height:70px;
background:transparent url(//oss.aliyuncs.com/vpncloud-a/assets/best_badge-7de5afabe5f75012b43c313ba807bd94.png) no-repeat top right}

