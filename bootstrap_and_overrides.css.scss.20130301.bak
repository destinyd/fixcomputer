@import "bootstrap";
ul, ol {
  margin-bottom: 20px;
  margin-left: 0;
  padding-left: 0;
  display: inline-block;
  list-style-type: none;
}
h1,h2,h3,h4,h5,h6{margin:0;font-weight:bold;font-style:normal}
h1{
  font-size:36px;
  color:#434343;
  padding-bottom:7px;
  line-height:38px
}
h2{
  font-size:30px;
  color:#515151;
  padding-bottom:7px;
  line-height:32px
}
h3{
  font-size:24px;
  color:#515151;
  padding-bottom:28px;
  line-height:26px
}
h4{
  font-size:18px;
  color:#424242;
  padding-bottom:7px;
  line-height:20px
}
h5{
  font-size:16px;
  color:#424242;
  padding-bottom:7px;
  line-height:18px
}
h6{
  font-size:14px;
  color:#2B2B2B;
  padding-bottom:7px;
  line-height:16px
}
@import "bootstrap-responsive";
body {
  font-family: "Helvetica Neue",Helvetica,"Heiti SC","Segoe UI","Microsoft YaHei",Arial,sans-serif;
  font-size: 13px;
  line-height: 20px;
  /*color: #333;*/
  color: #585858;
}

html, body {
  background-color: #f5f5f5;
  color: #585858;
  -webkit-font-smoothing: antialiased;
}

.clearfix {
  zoom: 1;
  clear: both;
}

