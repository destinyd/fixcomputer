<% @prices = Price.index %>
<table>
  <thead>
    <tr>
      <th>项目名称</th><th>价格</th><th>备注</th>
    </tr>
  </thead>
  <tbody>
    <% @prices.each do |p| %>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.human_price %></td>
        <td><%= p.plus %></td>
      </tr>
    <% end %>
  </tbody>
</table>

